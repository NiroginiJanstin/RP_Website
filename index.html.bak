            <div id="methodology" class="sub-section">
              <h2>Methodology</h2>
              <div class="row">
                <div id="project-scope-left" class="col-md-6 col-sm-12 pt-lg-5">
                  <div class="section-left">
                    <div class="section-figure">
                      <img src="./Images/other/architecture.png" width="100%"/>
